{
  "manifest_version": 2,

  "name": "Chrome Extension",
  "description": "Chrome Extension",
  "author": "",
  "version": "1.0.0",

  "browser_action": {
    "default_title": "Chrome Extension"
  },

  "background": {
    "page": "background.html",
    "persistent": true
  },

  "content_scripts": [{
    "js": ["content.js"],
    "run_at": "document_end",
    "matches": ["http://*/*", "https://*/*"]
  }],

  "web_accessible_resources": [
    "images/*"
  ],

  "icons": {
  },

  "permissions": [
    "storage"
  ]
}
